<div>
    <h2>Welcome, {{username}}.</h2>
</div>
<div>
    {{#if posts.length}}
    <div class="post-list">
        <h3>Current posts:</h3>
        {{#each posts as |post|}}
        <div>
            <h4 class="post-title">{{post.title}}</h4>
            <p>{{post.content}}</p>
            <button data-id="{{post.id}}">DELETE</button>
        </div>
        {{/each}}
    </div>
    {{/if}}
</div>

<script src="./js/dashboard.js"></script>